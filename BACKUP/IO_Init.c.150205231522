#include<iom16v.h>


void IO_Init()
{
	DDRA=0x00;
	PORTA=0xff;	
	DDRB=0x00;
	PORTB=0xff;
	DDRC=0xff;
	PORTC=0xff;
	DDRD=0x00;
	PORTD=0xff;
	DDRB|=0xff;
	PORTB&=~0x0f;
	DDRD|=(1<<PD3);
	PORTD&=~(1<<PD3);
	DDRD|=(1<<PD4);
	DDRD|=(1<<PD5);
	PORTD&=~(1<<PD4);
	PORTD&=~(1<<PD5);
	DDRD|=(1<<PD6);
	PORTD&=~(1<<PD6);
	DDRD|=(1<<PD7);
	PORTD|=(1<<PD7);
}